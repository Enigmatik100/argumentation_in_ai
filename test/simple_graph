digraph {
	graph [rankdir=LR]
	a [label=a fillcolor="#ff000042" style=filled]
	a [label=a]
	b [label=b]
	c [label=c fillcolor="#ff000042" style=filled]
	c [label=c]
	d [label=d]
	e [label=e fillcolor="#ff000042" style=filled]
	e [label=e]
	f [label=f]
	a -> b
	c -> b
	b -> c
	c -> d
	d -> e
	e -> f
}
